<launch>
  <rosparam file="$(find carla_ackermann_control)/config/settings.yaml" command="load" />
  <arg name="rolename"  />


  <node pkg="carla_ackermann_control" type="carla_ackermann_control_node.py" name="$(arg rolename)_carla_ackermann_control" output="screen">
    <param name="rolename" value="$(arg rolename)" />
  </node>
</launch>
